{
  "--name": "sample_tools_UI",
  "--workpath": "build",
  "--distpath": "dist",
  "--icon": {
    "Windows": "tools/UI/icons/sample_tools.ico",
    "Darwin": "tools/UI/icons/sample_tools.icns",
    "Linux": null
  },
  "--splash": {
    "Windows": "tools/UI/icons/sample_tools_splash.png",
    "Darwin": null,
    "Linux": "tools/UI/icons/sample_tools_splash.png"
  },
  "--onefile": {
    "Darwin": false,
    "Linux": true,
    "Windows": true
  },
  "--onedir": {
    "Darwin": true,
    "Windows": false,
    "Linux": false
  },
  "--noconfirm": true,
  "--noconsole": true,
  "--add-data": [
    "*.py:.",
    "tools:tools/"
  ],
  "--hidden-import": [
    "pandas",
    "mutagen",
    "chunkmuncher",
    "matplotlib",
    "sounddevice",
    "soundfile",
    "noisereduce",
    "soxr",
    "pyrubberband",
    "webcolors",
    "numpy",
    "scipy",
    "PyQt5",
    "PIL",
    "platformdirs",
    "getpass"
  ],
  "--collect-submodules": [
    "PyQt5.Qt",
    "PIL.Image",
    "PIL.ImageDraw",
    "matplotlib"
  ],
  "--exclude": [
    "tkinter",
    "pip",
    "pyinstaller"
  ],
  "--optimize": 2,
  "--version-file": {
    "Windows": "sample_tools_UI_version.txt"
  },
  "--noupx": true
}